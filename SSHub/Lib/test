
local LibAdapter = {}

local function Adapt()
    local Adapts = {}
    local Library = loadstring(game:GetObjects("rbxassetid://8245761742")[1].Source)("Pepsis World")
    
    function Adapt:CreateWindow(Name, Info)
        local InfoT = {}
        InfoT.Name = Name
        InfoT.Info = Info
        local Window = Library:CreateWindow({Name=InfoT.Name,Info=InfoT.Info,1})
        return InfoT
    end
    return Adapts
end

return LibAdapter
