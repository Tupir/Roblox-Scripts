
local LibAdapter = {}

function LibAdapter:Adapt()
    local Adapts = {}
    local Library = loadstring(game:GetObjects("rbxassetid://8245761742")[1].Source)("Pepsis World")

    function Adapts:CreateWindow(NameF, InfoF)
        local InfoT = {}
        InfoT.Name = NameF
        InfoT.Info = InfoF
        local Window = Library:CreateWindow({Name=InfoT.Name,Info=InfoT.Info,1})
        return InfoT
    end
    return Adapts
end

return LibAdapter
