if name:FindFirstChildOfClass('Humanoid') then
if name:FindFirstChild('Torso') then
range = (char.PrimaryPart.Position - name.PrimaryPart.Position).Magnitude
if range <15 and name.Name ~= player.Name then
spawn(function()
local args = {
    [1] = getrenv()._G.Pass,
    [2] = name,
    [3] = {
        ["HitTime"] = 0.2,
