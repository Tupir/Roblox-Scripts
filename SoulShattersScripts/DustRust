game:GetService("UserInputService").InputBegan:Connect(function(key, gc)
    if gc then return end
    if key.KeyCode == Enum.KeyCode["9"] then
if lv == 12 then
    dmg = 10
elseif lv == 11 then
    dmg = 9
elseif lv == 10 then
    dmg = 9
elseif lv == 9 then
    dmg = 8
elseif lv == 8 then
    dmg = 8
elseif lv == 7 then
    dmg = 7
elseif lv == 6 then
    dmg = 6
elseif lv == 5 then
    dmg = 5
elseif lv == 4 then
    dmg = 4
elseif lv == 3 then
    dmg = 3
elseif lv == 2 then
    dmg = 2
elseif lv == 1 then
    dmg = 1
else 
dmg = 10
end
print(dmg)
