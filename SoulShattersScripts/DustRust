player = game.Players.LocalPlayer
char = player.Character
for _,v in pairs(char:GetDescendants()) do
if v.Name == 'StaminaRegen' then
v:Destroy()
end
end

spawn(function()
while game.Players.LocalPlayer.Character.Data.Stamina.Value >= 600 do
wait(0.15)
game.Lighting.ColorCorrection.TintColor = Color3.fromRGB(255, 255, 255)
game.Lighting.Blur.Size = 0
end

while game.Players.LocalPlayer.Character.Data.Stamina.Value >= 600 do
wait(0.15)
game.Lighting.ColorCorrection.TintColor = Color3.fromRGB(255, 250, 250)
end

while game.Players.LocalPlayer.Character.Data.Stamina.Value >= 550 do
wait(0.15)
game.Lighting.ColorCorrection.TintColor = Color3.fromRGB(255, 240, 240)
end

while game.Players.LocalPlayer.Character.Data.Stamina.Value >= 500 do
wait(0.15)
game.Lighting.ColorCorrection.TintColor = Color3.fromRGB(255, 230, 230)
end

while game.Players.LocalPlayer.Character.Data.Stamina.Value >= 450 do
wait(0.15)
game.Lighting.ColorCorrection.TintColor = Color3.fromRGB(255, 220, 220)
end

while game.Players.LocalPlayer.Character.Data.Stamina.Value >= 400 do
wait(0.15)
game.Lighting.ColorCorrection.TintColor = Color3.fromRGB(255, 210, 210)
end

while game.Players.LocalPlayer.Character.Data.Stamina.Value >= 350 do
wait(0.15)
game.Lighting.ColorCorrection.TintColor = Color3.fromRGB(255, 200, 200)
game.Lighting.Blur.Size = 0
end

while game.Players.LocalPlayer.Character.Data.Stamina.Value >= 300 do
wait(0.15)
game.Lighting.ColorCorrection.TintColor = Color3.fromRGB(255, 190, 190)
game.Lighting.Blur.Size = 0.5
end

while game.Players.LocalPlayer.Character.Data.Stamina.Value >= 250 do
wait(0.15)
game.Lighting.ColorCorrection.TintColor = Color3.fromRGB(255, 180, 180)
game.Lighting.Blur.Size = 1
end

while game.Players.LocalPlayer.Character.Data.Stamina.Value >= 200 do
wait(0.15)
game.Lighting.ColorCorrection.TintColor = Color3.fromRGB(255, 170, 170)
game.Lighting.Blur.Size = 2
end

while game.Players.LocalPlayer.Character.Data.Stamina.Value >= 150 do
wait(0.15)
game.Lighting.ColorCorrection.TintColor = Color3.fromRGB(255, 150, 150)
game.Lighting.Blur.Size = 3
end
end)

-- Finish
wait(120)
