weld = Instance.new('Weld',sword2)
weld.Part0 = sword2
weld.Part1 = char['Left Arm']
sword2.Name = 'DusttrustSwordRight'
sword2.Color = Color3.fromRGB(98, 37, 209)
weld.C0 = CFrame.new(0.95,-1.7,-0)*CFrame.Angles(1.6,0,-1.6)
sword2.Transparency = 0
end
end




