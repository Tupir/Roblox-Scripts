local m = game.Players.LocalPlayer:GetMouse()
db = true
m.keyDown:connect(function(k)
	k = k:lower()
	if k == "m" then
		if db == true then
			local A_1 = 
				{
					[1] = getrenv()._G.Pass,
					[2] = "Blocking", 
					[3] = false
				}
			local Event = game:GetService("ReplicatedStorage").Remotes.Functions
			Event:InvokeServer(A_1)
			db = false
			wait(1)
			db = true
		end
	end
end)


